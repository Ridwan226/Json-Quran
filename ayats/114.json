      "ayats": [
        {
          "id": 6231,
          "surat_id": 114,
          "jus": 30,
          "ruku": 556,
          "no_ayat_inquran": 6231,
          "no_ayat_insurat": 1,
          "body_ayat_arb": "قُلْ أَعُوذُ بِرَبِّ النَّاسِ",
          "body_ayat_ltn_en": "Qul a'uzu birabbin naas",
          "body_ayat_ltn_in": "qul a'uudzu birabbi alnnaasi",
          "body_ayat_ltn_in_html": "qul a'uu<u>dz</u>u birabbi <strong>al</strong>nn<u>aa</u>s<strong>i</strong>",
          "invers_tajweed": "قُلْ أَعُوذُ بِرَبِّ [h:14683[ٱ][l[ل][g[نّ][p[َا]سِ",
          "translation_id": "Katakanlah, “Aku berlindung kepada Tuhannya manusia,",
          "translation_en": "Say, \"I seek refuge in the Lord of mankind,",
          "audio_praimary": "https://cdn.alquran.cloud/media/audio/ayah/ar.alafasy/6231",
          "audio_secondary": "https://cdn.islamic.network/quran/audio/128/ar.alafasy/6231.mp3",
          "tafsir_short": "Wahai Nabi Muhammad, katakanlah kepada umatmu, “Aku berlindung kepada Tuhan yang menciptakan, memelihara, dan mengurus manusia."
        },
        {
          "id": 6232,
          "surat_id": 114,
          "jus": 30,
          "ruku": 556,
          "no_ayat_inquran": 6232,
          "no_ayat_insurat": 2,
          "body_ayat_arb": "مَلِكِ النَّاسِ",
          "body_ayat_ltn_en": "Malikin naas",
          "body_ayat_ltn_in": "maliki alnnaasi",
          "body_ayat_ltn_in_html": "maliki <strong>al</strong>nn<u>aa</u>s<strong>i</strong>",
          "invers_tajweed": "مَلِكِ [h:14684[ٱ][l[ل][g[نّ][p[َا]سِ",
          "translation_id": "Raja manusia,",
          "translation_en": "The Sovereign of mankind.",
          "audio_praimary": "https://cdn.alquran.cloud/media/audio/ayah/ar.alafasy/6232",
          "audio_secondary": "https://cdn.islamic.network/quran/audio/128/ar.alafasy/6232.mp3",
          "tafsir_short": "Raja manusia, yang mengatur semua urusan mereka, dan Dia Mahakaya sehingga tidak membutuhkan mereka."
        },
        {
          "id": 6233,
          "surat_id": 114,
          "jus": 30,
          "ruku": 556,
          "no_ayat_inquran": 6233,
          "no_ayat_insurat": 3,
          "body_ayat_arb": "إِلَٰهِ النَّاسِ",
          "body_ayat_ltn_en": "Ilaahin naas",
          "body_ayat_ltn_in": "ilaahi alnnaasi",
          "body_ayat_ltn_in_html": "il<u>aa</u>hi <strong>al</strong>nn<u>aa</u>s<strong>i</strong>",
          "invers_tajweed": "إِلَ[n[ـٰ]هِ [h:14685[ٱ][l[ل][g[نّ][p[َا]سِ",
          "translation_id": "sembahan manusia,",
          "translation_en": "The God of mankind,",
          "audio_praimary": "https://cdn.alquran.cloud/media/audio/ayah/ar.alafasy/6233",
          "audio_secondary": "https://cdn.islamic.network/quran/audio/128/ar.alafasy/6233.mp3",
          "tafsir_short": "Sembahan manusia, Tuhan yang tidak ada sekutu bagi-Nya. Itulah Tuhan yang patut disembah dan dimintai perlindungan."
        },
        {
          "id": 6234,
          "surat_id": 114,
          "jus": 30,
          "ruku": 556,
          "no_ayat_inquran": 6234,
          "no_ayat_insurat": 4,
          "body_ayat_arb": "مِن شَرِّ الْوَسْوَاسِ الْخَنَّاسِ",
          "body_ayat_ltn_en": "Min sharril was waasil khannaas",
          "body_ayat_ltn_in": "min syarri alwaswaasi alkhannaasi",
          "body_ayat_ltn_in_html": "min syarri <strong>a</strong>lwasw<u>aa</u>si <strong>a</strong>lkhann<u>aa</u>s<strong>i</strong>",
